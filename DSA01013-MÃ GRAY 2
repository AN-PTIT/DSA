#include<bits/stdc++.h>
using namespace std;
int main(){
	int t;cin>>t;
	while(t--){
		string s;cin>>s;
		string s2=s;
		s2[0]=s[0];
		for(int i=1;i<s.size();i++){
			if(s[i]!=s2[i-1]) s2[i]='1';
			else s2[i]='0'; 
		}
		cout<<s2<<endl;
	}
} 
//quy tac:XOR dan xen 2 xau

